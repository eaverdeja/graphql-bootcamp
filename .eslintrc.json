{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "max-len": ["error", {"code": 80, "ignoreUrls": true}],
    "no-restricted-syntax": [0, "ForInStatement", "ForOfStatement"]
  }
}
